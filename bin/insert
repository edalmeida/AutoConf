#!/bin/bash

export PATH="${PATH}:/home/lbd/hadoop/ramiro/c3sl/AutoConf/bin/"

match=1
device=sda

cd /tmp/autoconf/jobs/
for machine in $(cat /tmp/out); do
	cd $machine
	{ for file in *.cpu;  do
        sys2csv.sh -s $file $machine $match
    done } &

	{ for file in *.disk; do
        sys2csv.sh -i $file $machine $match $device
    done } &

	{ for file in *.mem;  do
        sys2csv.sh -v $file $machine $match
    done } &
	cd ..
done
cd -
